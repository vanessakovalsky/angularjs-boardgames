{
  "name": "angular-js-boardgames",
  "private": true,
  "version": "0.0.0",
  "description": "A tutorial application for AngularJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angular/angular-phonecat.git"
  },
  "license": "MIT",
  "dependencies": {
    "angular": "1.7.x",
    "bootstrap": "3.3.x"
  },
  "devDependencies": {
    "http-server": "^0.11.1"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,bootstrap/dist,jquery/dist}/**/*\" app/lib -C",
    "prestart": "npm install",
    "start": "http-server ./app -a localhost -p 8000 -c-1"
  },
  "bugs": {
    "url": "https://github.com/angular/angular-phonecat/issues"
  },
  "homepage": "https://github.com/angular/angular-phonecat#readme",
  "main": "index.js",
  "author": ""
}
